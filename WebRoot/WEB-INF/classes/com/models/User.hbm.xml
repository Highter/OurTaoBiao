<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.models.User" table="user_table" catalog="mytaobiao" lazy="false" >
		<id name="userId" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="userName" length="50"/>
		<property name="passWord" length="20"/>
		<property name="MSN" length="50"/>
		<property name="qq" length="20"/>
		<property name="officePhone" length="20"/>
		<property name="familyPhone" length="20"/>
		<property name="cellPhone" length="20"/>
		<property name="email" length="50"/>
		<property name="passTip" length="50"/>
		<property name="passAnswer" length="50"/>
		<property name="lastLoginTime" type="timestamp"/>
		<property name="balance" type="float"/>
		<property name="consume" type="float"/>
		<property name="level" length="2"/>
		<property name="role" length="1"/>
	</class>
</hibernate-mapping>